{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"  // Specify Python version
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "scripts": {
    "postinstall": "pip install pymysql"  // Ensures pymysql is installed
  }
}
